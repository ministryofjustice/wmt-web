{# WMT0160: add new tiers #}
{% extends "includes/layout.njk" %}

{% block content %}

<main id="content" role="main">

{% include "includes/breadcrumbs.njk" %}

{% include "includes/validation-error-messages.njk" %}

{% include "includes/title-subtitle.njk" %}

{% include "includes/sub-nav.njk" %}

  <h2 class="govuk-heading-l">Caseload totals</h2>
  <div id="caseloadSummary" class="govuk-grid-row">              
  {% for caseType in caseloadDetails %}
    {% if caseType.displayName !== 'Overall' %}
      <div class="govuk-grid-column-one-third">
        <div id="{{ caseType.displayName.toLowerCase() }}Total" class="data">
          <span class="data-item bold-xlarge">{{ caseType.totalSummary }}</span>
          <span class="data-item bold-xsmall">{{ caseType.displayName }} cases</span>
        </div>
      </div>
    {% endif %}
  {% endfor %}
  </div>

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
    {% if canExportCaseload %}
      <a href="/probation/{{ organisationLevel }}/{{ linkId }}/{{ screen }}/caseload-csv" class="govuk-button sln-export floatRightBottom10" data-module="govuk-button">Export Caseload</a>
    {% endif %}
    </div>
  </div>

  <h2 class="govuk-heading-l">Caseload type breakdown</h2>
  <div class="js-tabs">
    <ul class="js-tabs-links">
      {% for caseType in caseloadDetails %}
	      <li><a class="govuk-link" name="{{ caseType.displayName.toLowerCase() }}" href="#{{ caseType.displayName.toLowerCase() }}" role="dynamic">{{caseType.displayName | title}}</a></li>
      {% endfor %}
    </ul>

  {% if organisationLevel === 'team' %}
    <div class="js-tabs-content">
    {% for caseType in caseloadDetails %}
      <div id="{{caseType.displayName.toLowerCase()}}">
        <h3 class="govuk-heading-m">{{ caseType.displayName | title }} caseload by tier</h3>

        <table id="team-caseload-data-table-{{caseType.displayName.toLowerCase()}}" class="govuk-table sln-table-caseload-{{caseType.displayName.toLowerCase()}} data-table">
          <thead>
            <tr>
              <th colspan="2" id="{{caseType.displayName}}_om">{{ childOrganisationLevelDisplayText }}</th>
              <th colspan="18" id="{{caseType.displayName}}_tiers">Case tiers</th>
            </tr>
            <tr>
              <th id="{{caseType.displayName}}_name">Name</th>
              <th id="{{caseType.displayName}}_grade">Grade</th>
              <th id="{{caseType.displayName}}_a3">A3</th>
              <th id="{{caseType.displayName}}_a2">A2</th>
              <th id="{{caseType.displayName}}_a1">A1</th>
              <th id="{{caseType.displayName}}_a0">A0</th>
              <th id="{{caseType.displayName}}_b3">B3</th>
              <th id="{{caseType.displayName}}_b2">B2</th>
              <th id="{{caseType.displayName}}_b1">B1</th>
              <th id="{{caseType.displayName}}_b0">B0</th>
              <th id="{{caseType.displayName}}_c3">C3</th>
              <th id="{{caseType.displayName}}_c2">C2</th>
              <th id="{{caseType.displayName}}_c1">C1</th>
              <th id="{{caseType.displayName}}_c0">C0</th>
              <th id="{{caseType.displayName}}_d3">D3</th>
              <th id="{{caseType.displayName}}_d2">D2</th>
              <th id="{{caseType.displayName}}_d1">D1</th>
              <th id="{{caseType.displayName}}_d0">D0</th>
              <th id="{{caseType.displayName}}_untiered">Untiered</th>              
              <th id="{{caseType.displayName}}_total">Total</th>
            </tr>
          </thead>

          <tbody class="">
            {% for member in caseType.array %}
              <tr>
                <td headers="{{caseType.displayName}}_om {{caseType.displayName}}_name"><a class="govuk-link" href="/{{ workloadType }}/{{ childOrganisationLevel }}/{{ member.linkId }}">{{ member.name }}</a></td>
                <td class="align-centre" headers="{{caseType.displayName}}_om {{caseType.displayName}}_grade">{{ member.grade }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a3">{{ member.a3 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a2">{{ member.a2 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a1">{{ member.a1 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a0">{{ member.a0 }}</td>

                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b3">{{ member.b3 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b2">{{ member.b2 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b1">{{ member.b1 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b0">{{ member.b0 }}</td>

                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c3">{{ member.c3 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c2">{{ member.c2 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c1">{{ member.c1 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c0">{{ member.c0 }}</td>

                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d3">{{ member.d3 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d2">{{ member.d2 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d1">{{ member.d1 }}</td>
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d0">{{ member.d0 }}</td>

                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_untiered">{{ member.untiered }}</td>                
                <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_total">{{ member.totalCases }}</td>
              </tr>
            {% endfor %}
          </tbody>
          <tfoot>
            <tr>
              <td colspan="2" headers="{{caseType.displayName}}_grade {{caseType.displayName}}_name {{caseType.displayName}}_om">Total</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a3">{{ caseType.array.totals.a3 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a2">{{ caseType.array.totals.a2 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a1">{{ caseType.array.totals.a1 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_a0">{{ caseType.array.totals.a0 | round(1) }}</td>

              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b3">{{ caseType.array.totals.b3 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b2">{{ caseType.array.totals.b2 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b1">{{ caseType.array.totals.b1 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_b0">{{ caseType.array.totals.b0 | round(1) }}</td>

              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c3">{{ caseType.array.totals.c3 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c2">{{ caseType.array.totals.c2 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c1">{{ caseType.array.totals.c1 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_c0">{{ caseType.array.totals.c0 | round(1) }}</td>

              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d3">{{ caseType.array.totals.d3 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d2">{{ caseType.array.totals.d2 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d1">{{ caseType.array.totals.d1 | round(1) }}</td>
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_d0">{{ caseType.array.totals.d0 | round(1) }}</td>

              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_untiered">{{ caseType.array.totals.untiered | round(1) }}</td>   
              <td headers="{{caseType.displayName}}_tiers {{caseType.displayName}}_total" >{{ caseType.array.totals.overall | round(1) }}</td>   
            </tr>
          </tfoot>
        </table>
      </div>
    {% endfor %}
    </div>

  {% else %}
    <div class="js-tabs-content">     
    {% for caseType in caseloadDetails %}
    {% if caseType.displayName === 'Overall' %}
      <div id="{{caseType.displayName.toLowerCase()}}">
        <h3 class="govuk-heading-m">{{ caseType.displayName | title }} caseload</h3>        
        <table id="overall-caseload-data-table" class="govuk-table sln-table-caseload-overall-summary data-table">
          <thead>
            <tr>
              <th colspan="1" id="org">{{ childOrganisationLevelDisplayText }}</th>
              <th colspan="4" id="totals">Cases totals</th>
            </tr>
            <tr>
              <th id="name">Name</th>
              <th id="community-total" >Community Total</th>
              <th id="license-total">License Total</th>
              <th id="custody-total">Custody Total</th>
              <th id="total">Total</th>
            </tr>
          </thead>
          <tbody class="">
          {% for org in caseType.totalSummary %}
            <tr>
              {% if org.name === 'Total' %}
              <td headers="name org">{{ org.name }}</td>
              {% else %}
              <td headers="name org"><a class="govuk-link" href="/{{ workloadType }}/{{ childOrganisationLevel }}/{{ org.linkId }}">{{ org.name }}</a></td>
              {% endif %}
              <td headers="community-total totals">{{ org.communityTotalCases }}</td>
              <td headers="license-total totals">{{ org.licenseTotalCases }}</td>
              <td headers="custody-total totals">{{ org.custodyTotalCases }}</td>
              <td headers="total totals">{{ org.totalCases }}</td>
            </tr>
          {% endfor %}
          </tbody>
          <tfoot>
            <tr>
              <td headers="name org">Total</td>
              <td headers="community-total totals">{{caseType.totalsRow.totalCommunity}}</td>
              <td headers="license-total totals">{{caseType.totalsRow.totalLicense}}</td>
              <td headers="custody-total totals">{{caseType.totalsRow.totalCustody}}</td>
              <td headers="total totals">{{caseType.totalsRow.totalTotalCases}}</td>
            </tr>
          </tfoot> 
        </table>
        <br/>
        <h3 class="govuk-heading-m">{{ caseType.displayName | title }} caseload by grade</h3>
        <table id="overall-grade-caseload-data-table" class="govuk-table sln-table-caseload-by-grade data-table">
          <thead>
            <tr>
              <th colspan="2" id="{{caseType.displayName}}_om_grade">{{ childOrganisationLevelDisplayText }}</th>
              <th colspan="18" id="{{caseType.displayName}}_tiers_grade">Case tiers</th>
            </tr>
            <tr>
              <th id="{{caseType.displayName}}_name_grade">Name</th>
              <th id="{{caseType.displayName}}_grade_grade">Grade</th>
              <th id="{{caseType.displayName}}_a3_grade">A3</th>
              <th id="{{caseType.displayName}}_a2_grade">A2</th>
              <th id="{{caseType.displayName}}_a1_grade">A1</th>
              <th id="{{caseType.displayName}}_a0_grade">A0</th>

              <th id="{{caseType.displayName}}_b3_grade">B3</th>
              <th id="{{caseType.displayName}}_b2_grade">B2</th>
              <th id="{{caseType.displayName}}_b1_grade">B1</th>
              <th id="{{caseType.displayName}}_b0_grade">B0</th>

              <th id="{{caseType.displayName}}_c3_grade">C3</th>
              <th id="{{caseType.displayName}}_c2_grade">C2</th>
              <th id="{{caseType.displayName}}_c1_grade">C1</th>
              <th id="{{caseType.displayName}}_c0_grade">C0</th>

              <th id="{{caseType.displayName}}_d3_grade">D3</th>
              <th id="{{caseType.displayName}}_d2_grade">D2</th>
              <th id="{{caseType.displayName}}_d1_grade">D1</th>
              <th id="{{caseType.displayName}}_d0_grade">D0</th>

              <th id="{{caseType.displayName}}_untiered_grade">Untiered</th>          
              <th id="{{caseType.displayName}}_overall_grade">Overall</th>            
            </tr>
          </thead>
          <tbody class="">
            {% for member in caseType.array.detailsPercentages %}
              {% for i in range(0, member.grades.length) %}
                <tr>
                  {% if i === 0 %}
                    <td rowspan="{{ member.grades.length }}" headers="{{caseType.displayName}}_om_grade {{caseType.displayName}}_name_grade"><a class="govuk-link" href="/{{ workloadType }}/{{ childOrganisationLevel }}/{{ member.linkId }}">{{ member.name }}</a></td>
                  {% endif %}
                  <td class="align-centre" headers="{{caseType.displayName}}_om_grade {{caseType.displayName}}_grade_grade">{{ member.grades[i].grade }}</td>            
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a3_grade">{{ member.grades[i].a3 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a2_grade">{{ member.grades[i].a2 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a1_grade">{{ member.grades[i].a1 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a0_grade">{{ member.grades[i].a0 | round(1) }}%</td>

                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b3_grade">{{ member.grades[i].b3 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b2_grade">{{ member.grades[i].b2 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b1_grade">{{ member.grades[i].b1 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b0_grade">{{ member.grades[i].b0 | round(1) }}%</td>

                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c3_grade">{{ member.grades[i].c3 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c2_grade">{{ member.grades[i].c2 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c1_grade">{{ member.grades[i].c1 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c0_grade">{{ member.grades[i].c0 | round(1) }}%</td>

                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d3_grade">{{ member.grades[i].d3 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d2_grade">{{ member.grades[i].d2 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d1_grade">{{ member.grades[i].d1 | round(1) }}%</td>
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d0_grade">{{ member.grades[i].d0 | round(1) }}%</td>

                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_untiered_grade">{{ member.grades[i].untiered | round(1) }}%</td>            
                  <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_overall_grade" >{{ member.grades[i].totalCases | round(1) }}%</td>              
                </tr>
              {% endfor %}
            {% endfor %}
          </tbody>
          <tfoot>
            <tr>
              <td rowspan="{{caseType.array.totals | length}}" headers="{{caseType.displayName}}_name_grade {{caseType.displayName}}_om_grade">Average</td>
              {% for grade, total in caseType.array.percentageTotals %}
                <td class="align-centre" headers="{{caseType.displayName}}_om_grade {{caseType.displayName}}_grade_grade">{{ grade }}</td> 
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a3_grade">{{ total.a3  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a2_grade">{{ total.a2  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a1_grade">{{ total.a1  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_a0_grade">{{ total.a0 | round(1) }}%</td>

                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b3_grade">{{ total.b3  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b2_grade">{{ total.b2  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b1_grade">{{ total.b1  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_b0_grade">{{ total.b0 | round(1) }}%</td>

                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c3_grade">{{ total.c3  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c2_grade">{{ total.c2  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c1_grade">{{ total.c1  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_c0_grade">{{ total.c0 | round(1) }}%</td>

                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d3_grade">{{ total.d3  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d2_grade">{{ total.d2  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d1_grade">{{ total.d1  | round(1) }}%</td>
                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_d0_grade">{{ total.d0 | round(1) }}%</td>

                <td headers="{{caseType.displayName}}_tiers_grade {{caseType.displayName}}_untiered_grade">{{ total.untiered | round(1) }}%</td>   
                <td headers="{{caseType.displayName}}_overall_grade {{caseType.displayName}}_tiers_grade" >{{ total.totalCases | round(1) }}%</td>
            </tr>
              <tr>
              {% endfor %}
        </tfoot>
      </table>
      </div>
    {% else %}
      <div id="{{caseType.displayName.toLowerCase()}}">
        <h3 class="govuk-heading-m">{{ caseType.displayName | title }} caseload by tier</h3>
        <table id="{{caseType.displayName}}_tier-caseload-data-table" class="govuk-table sln-table-caseload-{{caseType.displayName.toLowerCase()}} data-table">
          <thead>
            <tr>
              <th colspan="2" id="{{caseType.displayName}}_om_caseload">{{ childOrganisationLevelDisplayText }}</th>
              <th colspan="18" id="{{caseType.displayName}}_tiers_caseload">Case tiers</th>
            </tr>
            <tr>
              <th id="{{caseType.displayName}}_name_caseload">Name</th>
              <th id="{{caseType.displayName}}_grade_caseload">Grade</th>

              <th id="{{caseType.displayName}}_a3_caseload">A3</th>
              <th id="{{caseType.displayName}}_a2_caseload">A2</th>
              <th id="{{caseType.displayName}}_a1_caseload">A1</th>
              <th id="{{caseType.displayName}}_a0_caseload">A0</th>
  
              <th id="{{caseType.displayName}}_b3_caseload">B3</th>
              <th id="{{caseType.displayName}}_b2_caseload">B2</th>
              <th id="{{caseType.displayName}}_b1_caseload">B1</th>
              <th id="{{caseType.displayName}}_b0_caseload">B0</th>
  
              <th id="{{caseType.displayName}}_c3_caseload">C3</th>
              <th id="{{caseType.displayName}}_c2_caseload">C2</th>
              <th id="{{caseType.displayName}}_c1_caseload">C1</th>
              <th id="{{caseType.displayName}}_c0_caseload">C0</th>
  
              <th id="{{caseType.displayName}}_d3_caseload">D3</th>
              <th id="{{caseType.displayName}}_d2_caseload">D2</th>
              <th id="{{caseType.displayName}}_d1_caseload">D1</th>
              <th id="{{caseType.displayName}}_d0_caseload">D0</th>

              <th id="{{caseType.displayName}}_untiered_caseload">Untiered</th>              
              <th id="{{caseType.displayName}}_overall_caseload">Overall</th>            
            </tr>
          </thead>
          <tbody class="">
            {% for member in caseType.array.details %}
            {% for i in range(0, member.grades.length) %}
              <tr>
                {% if i === 0 %}
                <td rowspan="{{ member.grades.length }}" headers="{{caseType.displayName}}_om_caseload {{caseType.displayName}}_name_caseload"><a class="govuk-link" href="/{{ workloadType }}/{{ childOrganisationLevel }}/{{ member.linkId }}">{{ member.name }}</a></td>
                {% endif %}
                <td class="align-centre {% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_om_caseload {{caseType.displayName}}_grade_caseload">{{ member.grades[i].grade }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_a3_caseload">{{ member.grades[i].a3 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_a2_caseload">{{ member.grades[i].a2 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_a1_caseload">{{ member.grades[i].a1 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_a0_caseload">{{ member.grades[i].a0 | round(1) }}</td>

                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_b3_caseload">{{ member.grades[i].b3 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_b2_caseload">{{ member.grades[i].b2 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_b1_caseload">{{ member.grades[i].b1 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_b0_caseload">{{ member.grades[i].b0 | round(1) }}</td>

                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_c3_caseload">{{ member.grades[i].c3 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_c2_caseload">{{ member.grades[i].c2 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_c1_caseload">{{ member.grades[i].c1 | round(1) }}</td>
                <td class="{% if member.grades[i].grade == 'Total' %} bold {% endif %}" headers="{{caseType.displayName}}_tiers_caseload {{caseType.displayName}}_c0_caseload">{{ member.grades[i].c0 | round(1) }}</td>

