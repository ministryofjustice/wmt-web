    {% if organisationLevel === 'team' %}
      <tbody>
        {% for member in capacityBreakdown %}
        
          {% if loop.last %}
        </tbody>
        <tfoot>
            <tr>
          <td colspan="2" headers="capacityGrade capacityName org">{{ member.name }}</td>
          <td headers="capacity breakdown">{{ member.capacity | round(1) }}%</td>
