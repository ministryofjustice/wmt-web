          <td headers="breakdown cmsPoints">{{ member.grades[i].cmsPoints }}</td>
        </tr>
        {% endfor %}
        {% if loop.last %}
    </tbody>
    <tfoot>
        <tr>
            <td colspan="2" headers="capacityGrade capacityName org">{{ member.name }}</td>
            <td headers="capacity breakdown">{{ member.capacity | round(1) }}%</td>
            <td headers="cases breakdown">{{ member.totalCases | round(1) }}</td>
