          <td headers="inactiveHeader unpaidWork">{{ member.grades[i].upw }}</td>
          <td headers="inactiveHeader overdueTermination">{{ member.grades[i].ot }}</td>
          <td headers="inactiveHeader suspendedLifers">{{ member.grades[i].sl }}</td>
          <td headers="inactiveHeader suspendedSentenceOrders">{{ member.grades[i].sso }}</td>
        </tr>
        {% endfor %}
        {% if loop.last %}
    </tbody>
    <tfoot>
        <tr>
            <td colspan="2" headers="grade name organisationLevel">{{ member.name }}</td>
            <td headers="inactiveHeader outstandingWarrants">{{ member.totalOW }}</td>
            <td headers="inactiveHeader unpaidWork">{{ member.totalUPW }}</td>
            <td headers="inactiveHeader overdueTermination">{{ member.totalOT }}</td>
            <td headers="inactiveHeader suspendedLifers">{{ member.totalSL }}</td>
            <td headers="inactiveHeader suspendedSentenceOrders">{{ member.totalSSO }}</td>
        </tr>
    </tfoot>
        {% endif %}
      {% endfor %}
    
    {% endif %}
  </table>
  {% endif %}
</main>

